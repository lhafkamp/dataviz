    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <hr>
                <div class="btn btn-primary loadReal">LOAD ALL REAL DATA</div>
                <div class="btn btn-primary loadReal-NL">LOAD NL REAL DATA</div>
